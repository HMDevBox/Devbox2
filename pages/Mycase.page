<apex:page controller="mycases">
  <apex:pageBlock title="My Cases">
        <apex:pageBlockTable value="{!lstcase}" var="w">
            <apex:column headerValue="Case Number" value="{!w.CaseNumber}"/>
            <apex:column headerValue="Owner" value="{!w.Owner.Name}"/>
            <apex:column headerValue="Subject" value="{!w.Subject}"/>
            <apex:column headerValue="Status" value="{!w.Status}"/>            
       </apex:pageBlockTable>
  </apex:pageBlock>     
</apex:page>